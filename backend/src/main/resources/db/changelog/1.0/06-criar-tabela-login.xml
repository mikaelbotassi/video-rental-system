<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <changeSet id="1" author="breno">
        <createTable tableName="user">
            <column name="login" type="varchar(255)"><constraints primaryKey="true" primaryKeyName="LOGIN_FK"></constraints> </column>
            <column name="password" type="varchar(255)"><constraints nullable="false"/></column>
        </createTable>
    </changeSet>
</databaseChangeLog>
